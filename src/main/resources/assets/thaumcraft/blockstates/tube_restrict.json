{    
    "multipart": [
        {   "when": { "north": "true" },
            "apply": { "model": "thaumcraft:block/tube_side_restrict", "y": 180, "x": 90 }
        },        
        {   "when": { "south": "true" },
            "apply": { "model": "thaumcraft:block/tube_side_restrict", "x": 90 }
        },
        {   "when": { "east": "true" },
            "apply": { "model": "thaumcraft:block/tube_side_restrict", "y": 270, "x": 90 }
        },
        {   "when": { "west": "true" },
            "apply": { "model": "thaumcraft:block/tube_side_restrict", "y": 90, "x": 90 }
        },
        {   "when": { "up": "true" },
            "apply": { "model": "thaumcraft:block/tube_side_restrict", "x": 180 }
        },
        {   "when": { "down": "true" },
            "apply": { "model": "thaumcraft:block/tube_side_restrict" }
        },
        {
            "apply": { "model": "thaumcraft:block/tube_core"}
        }
    ]
}
